    "pages/deviceinfo/deviceinfo",
    "pages/id-verify/id-verify",
    "pages/barcode/barcode",
    "pages/qrcode/qrcode",
    "pages/scan/scan",
    "pages/screentest/screentest",
    "pages/multitouch/multitouch",
    "pages/random/random",
    "pages/converter/converter",
    "pages/unit-converter/unit-converter",
    "pages/calculator/calculator",
    "pages/mbti/mbti",
    "pages/psychology-test/psychology-test",
    "pages/psychology-test/bdi/bdi",
    "pages/psychology-test/sas/sas",
    "pages/psychology-test/scl90/scl90",
